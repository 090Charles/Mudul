
@using MudulProject.Models;
@{
    
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var List1 = ViewData["matriculas"] as List<_matriculas>;
}

<h2>Matriculas</h2>


<table class="table">
    <tr>
        <th>
            Fecha
        </th>
        <th>
           Cuenta
        </th>
        <th>
            Nombre
        </th>
        <th>
            Tipo Matricula
        </th>
        <th>
            Campus
        </th>
        <th>
            Lugar Campus
        </th>
        <th></th>
    </tr>

@foreach (var item in List1) {
    <tr>

        <td>
          @item.Fecha
        </td>
        <td>
          @item.Cuenta
        </td>
        <td>
           @item.Nombre
        </td>
        <td>
            @item.TipoMatricula
        </td>
        <td>
          @item.Campus
        </td>
        <td>
            @item.LugarCampus
        </td>
        <td>
            @Html.ActionLink("Ver Clases", "Edit", new { id=item.Id }) 
        </td>
    </tr>
}

</table>

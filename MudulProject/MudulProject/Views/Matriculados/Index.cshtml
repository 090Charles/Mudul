
@using MudulProject.Models;
@{
    
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var List1 = ViewData["matriculas"] as List<_matriculas>;
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Fecha
        </th>
        <th>
           Cuenta
        </th>
        <th>
            Nombre
        </th>
        <th>
            Tipo Matricula
        </th>
        <th>
            Campus
        </th>
        <th>
            Lugar Campus
        </th>
        <th></th>
    </tr>

@foreach (var item in List1) {
    <tr>
        <td>
           item.Fecha
        </td>
        <td>
          item.NumberAccountId_Usuarios
        </td>
        <td>
          item.Id_TipoMatricula
        </td>
        <td>
           item.Id_MetodosDePago
        </td>
        <td>
            item.Id_Campus
        </td>
        <td>
          item.Total
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
